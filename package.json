{
  "name": "openresumebuilder",
  "version": "1.0.0",
  "description": "OpenResumeBuilder is resume building website using MERN stack.",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "installpackages": "npm run installbackend && npm run installlandingpage",
    "installbackend": "npm install",
    "installlandingpage": "cd ./apps/landingpage && npm install",
    "buildapps": "npm run buildlandingpage",
    "buildlandingpage": "cd ./apps/landingpage && npm run build",
    "dev": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2"
  }
}
