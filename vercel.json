{
  "buildCommand": "npm run build",
  "outputDirectory": "./frontend/dist",
  "installCommand": "npm run installpackages",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/admin/$1"
    },
    {
      "source": "/client/(.*)",
      "destination": "/client/$1"
    }
  ]
}
