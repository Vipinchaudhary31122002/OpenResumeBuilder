{
  "builds": [
    {
      "src": "frontend/*",
      "use": "@vercel/vite",
      "config": {
        "buildCommand": "npm run buildfronted",
        "outputDirectory": "./frontend/dist"
      }
    }
  ],
  "installCommand": "npm installpackages",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
