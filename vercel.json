{
  "buildCommand": "npm run build",
  "outputDirectory": "./frontend/dist",
  "installCommand": "npm run installpackages",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/admin/dist/$1"
    },
    {
      "source": "/client/(.*)",
      "destination": "/client/dist/$1"
    }
  ]
}
