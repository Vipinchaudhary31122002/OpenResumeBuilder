{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "./frontend/dist",
  "installCommand": "npm run installpackages",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "builds": [
    {
      "src": "./backend/server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./backend/server.js"
    }
  ]
}
